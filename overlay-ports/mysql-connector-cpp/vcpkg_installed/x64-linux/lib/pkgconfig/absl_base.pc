prefix=${pcfiledir}/../..
exec_prefix=${prefix}
libdir=${prefix}/lib
includedir=${prefix}/include

Name: absl_base
Description: Abseil base library
URL: https://abseil.io/
Version: 20250512

Libs: "-L${libdir}" -labsl_base -lrt
Requires: absl_atomic_hook = 20250512, absl_base_internal = 20250512, absl_config = 20250512, absl_core_headers = 20250512, absl_dynamic_annotations = 20250512, absl_log_severity = 20250512, absl_nullability = 20250512, absl_raw_logging_internal = 20250512, absl_spinlock_wait = 20250512, absl_type_traits = 20250512
Cflags: "-I${includedir}" -DNOMINMAX
